package cn.tju.scs;

public class Triangle {

	 private int a;
	 private int b;
	 private int c;
	public Triangle(){
		
	}
	
	public int ifTriangle(int x,int y,int z){
		//等边三角形返回1
		if(x==y&&y==z){
			return 1;
		}
		//等腰三角形返回2
		else if((x==y&&y!=z)||(y==z&&z!=x)||(x==z&&x!=y)){
			return 2;
		}
		//斜边三角形返回3
		else
			return 3;
	}

}
